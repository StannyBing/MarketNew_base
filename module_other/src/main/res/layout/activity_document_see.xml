<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/white"
              android:orientation="vertical">

    <com.zx.module_library.view.ToolBarView
        android:id="@+id/toobar_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.zx.module_library.view.ToolBarView>


    <WebView
        android:id="@+id/wv_documentsee"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layerType="hardware"
        ></WebView>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btn_fill_document"
            style="@style/MySubmitButton"
            android:layout_weight="1"
            android:text="@string/fill_document"/>

        <Button
            android:id="@+id/btn_print_document"
            style="@style/MySubmitButton"
            android:layout_weight="1"
            android:text="@string/print_document"/>
    </LinearLayout>

</LinearLayout>