<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.zx.module_library.view.ToolBarView
        android:id="@+id/toobar_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.zx.module_library.view.ToolBarView>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_10">

        <WebView
            android:id="@+id/wv_documentsee"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/dp_80"
            android:layerType="hardware"></WebView>

        <TextView
            android:id="@+id/tv_fill_document"
            android:layout_width="@dimen/dp_140"
            android:layout_height="@dimen/dp_50"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_15"
            android:background="@color/blue8c"
            android:gravity="center"
            android:text="@string/fill_document"
            android:textSize="@dimen/text_size_big" />

        <TextView
            android:id="@+id/tv_print_document"
            android:layout_width="@dimen/dp_140"
            android:layout_height="@dimen/dp_50"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_15"
            android:background="@color/blue8c"
            android:gravity="center"
            android:text="@string/print_document"
            android:textSize="@dimen/text_size_big" />
    </RelativeLayout>
</LinearLayout>